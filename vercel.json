{
  "version": 2,
  "builds": [
    { "src": "Portfolio-waad/package.json", "use": "@vercel/static-build" },
    { "src": "portfolio_backend/api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/portfolio_backend/api/index.js" },
    { "src": "/(.*)", "dest": "/Portfolio-waad/build/$1" }
  ]
}